{
  "name": "theoldnet-webring",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "ENVIRONMENT=development ts-node-dev ./src/server.ts",
    "dev": "ENVIRONMENT=development ts-node-dev --respawn ./src/server.ts",
    "tsc": "tsc",
    "build": "rimraf dist/ && yarn tsc",
    "start:prod": "ENVIRONMENT=production node ./dist/server.js",
    "start:prod2": "ENVIRONMENT=production ts-node-dev ./src/server.ts",
    "deploy": "git pull && pm2 stop server && yarn build && pm2 start server"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "body-parser": "^1.20.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-recaptcha": "^5.1.0",
    "express-useragent": "^1.0.15",
    "jsonwebtoken": "^8.5.1",
    "luxon": "^2.3.1",
    "md5": "^2.3.0",
    "node-fetch": "^3.2.3",
    "rimraf": "^3.0.2",
    "sequelize": "^6.19.0",
    "sqlite3": "^5.0.2",
    "uglify-js": "^3.15.4",
    "vash": "^0.13.0",
    "yaml": "^2.0.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.19.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/express-useragent": "^1.0.2",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/luxon": "^2.3.1",
    "@types/md5": "^2.3.2",
    "@types/node": "^17.0.23",
    "@types/node-fetch": "^2.6.1",
    "@types/sequelize": "^4.28.11",
    "@types/uglify-js": "^3.13.2",
    "@types/yaml": "^1.9.7",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  }
}
